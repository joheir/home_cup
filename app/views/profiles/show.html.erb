<div class="wrapper">
  <div class="progressoverview">
      <h3>Progress</h3>
    <%= link_to profile_profile_tasks_path(@profile) do %>
      <h6>Personal progress: <%= @profile.progress %>%</h6>
    <% end %>
    <h6>Family progress: 20%</h6>
  </div>
<hr>
  <div class="meals">
    <h3>What are we eating?</h3>
    <%= link_to profile_meals_path(@profile) do %>
    <div class="meal-card-dash">
      <img src="https://images.unsplash.com/photo-1546069901-ba9599a7e63c?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxzZWFyY2h8Mnx8Zm9vZHxlbnwwfHwwfHw%3D&auto=format&fit=crop&w=500&q=60"/>
      <div class="meal-card-infos">
        <div>
          <h2>Noodles</h2>
          <p>Short description here!</p>
        </div>
      </div>
    </div>
    <% end %>
  </div>
<hr>
  <div class="packing-list">
    <h3>My packing list</h3>

  </div>
</div>
