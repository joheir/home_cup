<div id="<%= dom_id task %>">
  <p>
    <strong>Title:</strong>
    <%= task.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= task.description %>
  </p>

  <p>
    <strong>State:</strong>
    <%= task.min_age %>
  </p>

  <p>
    <strong>Deleted at:</strong>
    <%= task.weekday %>
  </p>

</div>
